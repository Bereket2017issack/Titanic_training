CREATE TABLE datastore
             (
                          RowID         INTEGER
                        , OrderID       INTEGER
                        , OrderDate     TEXT
                        , ShipDate      TEXT
                        , ShipMode      INTEGER
                        , CustomerID    TEXT
                        , CustomerName  TEXT
                        , Segment       TEXT
                        , City          TEXT
                        , State         TEXT
                        , Country       TEXT
                        , PostalCode    TEXT
                        , Market        TEXT
                        , Region        TEXT
                        , ProductID     TEXT
                        , Category      TEXT
                        , SuCategory    TEXT
                        , ProductName   TEXT
                        , Sales         INTEGER
                        , Quantity      INTEGER
                        , Discount      INTEGER
                        , Profit        INTEGER
                        , ShippingCost  INTEGER
                        , OrderPriority TEXT
             )
;